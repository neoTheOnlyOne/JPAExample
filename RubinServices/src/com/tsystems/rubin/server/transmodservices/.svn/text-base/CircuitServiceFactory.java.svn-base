package com.tsystems.rubin.server.transmodservices;

import com.tsystems.rubin.server.dal.DAOFactory;
import com.tsystems.rubin.server.transmodservices.impl.CircuitServiceImpl;

/**
 * A factory for creating CircuitService objects.
 */
public class CircuitServiceFactory {
	/**
	 * Gets the Circuit service.
	 * 
	 * @param daoFactory the dao factory
	 * 
	 * @return the Carrier service
	 */
	public static final CircuitService getCircuitService(DAOFactory daoFactory) {
        return new CircuitServiceImpl(daoFactory);
    }
}
