package com.tsystems.rubin.server.transmodservices;

import com.tsystems.rubin.server.dal.DAOFactory;
import com.tsystems.rubin.server.transmodservices.impl.CapacityServiceImpl;

/**
 * A factory for creating CapacityService objects.
 */
public class CapacityServiceFactory {

	/**
	 * Gets the Capacity service.
	 * 
	 * @param daoFactory
	 *            the dao factory
	 * 
	 * @return the Capacity service
	 */
	public static final CapacityService getCapacityService(DAOFactory daoFactory) {
		return new CapacityServiceImpl(daoFactory);
	}

}
