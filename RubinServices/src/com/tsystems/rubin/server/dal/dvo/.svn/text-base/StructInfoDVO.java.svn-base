package com.tsystems.rubin.server.dal.dvo;

import java.io.Serializable;

public class StructInfoDVO implements Serializable{
	/** The constant serialVersionUID. */
	private static final long serialVersionUID = 1L;

	private Long id;

	private String designation;

	private String remark;
	
    private Long structInfoTypeId;
	
	private String structInfoTypedesignation;
	
	private Long transportverbindunid;
	
	public StructInfoDVO(Long id, String designation, String remark, Long structInfoTypeId,
			String structInfoTypedesignation, Long transportverbindunid) {
		this.id = id;
		this.designation = designation;
		this.remark = remark;
		this.structInfoTypeId = structInfoTypeId;
		this.structInfoTypedesignation = structInfoTypedesignation;
		this.transportverbindunid = transportverbindunid;
	}

	/**
	 * @return the id
	 */
	public long getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(long id) {
		this.id = id;
	}

	/**
	 * @return the designation
	 */
	public String getDesignation() {
		return designation;
	}

	/**
	 * @param designation the designation to set
	 */
	public void setDesignation(String designation) {
		this.designation = designation;
	}

	/**
	 * @return the remark
	 */
	public String getRemark() {
		return remark;
	}

	/**
	 * @param remark the remark to set
	 */
	public void setRemark(String remark) {
		this.remark = remark;
	}

	/**
	 * @return the structInfoTypeId
	 */
	public Long getStructInfoTypeId() {
		return structInfoTypeId;
	}

	/**
	 * @param structInfoTypeId the structInfoTypeId to set
	 */
	public void setStructInfoTypeId(Long structInfoTypeId) {
		this.structInfoTypeId = structInfoTypeId;
	}

	/**
	 * @return the structInfoTypedesignation
	 */
	public String getStructInfoTypedesignation() {
		return structInfoTypedesignation;
	}

	/**
	 * @param structInfoTypedesignation the structInfoTypedesignation to set
	 */
	public void setStructInfoTypedesignation(String structInfoTypedesignation) {
		this.structInfoTypedesignation = structInfoTypedesignation;
	}

	/**
	 * @return the transportverbindunid
	 */
	public Long getTransportverbindunid() {
		return transportverbindunid;
	}

	/**
	 * @param transportverbindunid the transportverbindunid to set
	 */
	public void setTransportverbindunid(Long transportverbindunid) {
		this.transportverbindunid = transportverbindunid;
	}
	
	
}
