package com.tsystems.rubin.server.dal.entities;

import java.io.Serializable;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="INFO_BEARBEITER_SECU")
public class InfoBearbeiterSecu implements Serializable {
	@Id
	@Column(name="BS_BEARB_SECU_ID")
	private Long id;

	@Column(name="BS_BEARB_ARB_ORT")
	private String bsBearbArbOrt;

	@Column(name="BS_BEARB_FAM_NAME")
	private String bsBearbFamName;

	@Column(name="BS_BEARB_KENNUNG")
	private String bsBearbKennung;

	@Column(name="BS_BEARB_KUERZEL")
	private String bsBearbKuerzel;

	@Column(name="BS_BEARB_TELEFON")
	private String telephoneNumber;

	@Column(name="BS_BEARB_VORNAME")
	private String firstName;

	@Column(name="BS_PASSWORD")
	private String password;

	private static final long serialVersionUID = 1L;

	public InfoBearbeiterSecu() {
		super();
	}

	/**
	 * @return the id
	 */
	public Long getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(Long id) {
		this.id = id;
	}

	/**
	 * @return the bsBearbArbOrt
	 */
	public String getBsBearbArbOrt() {
		return bsBearbArbOrt;
	}

	/**
	 * @param bsBearbArbOrt the bsBearbArbOrt to set
	 */
	public void setBsBearbArbOrt(String bsBearbArbOrt) {
		this.bsBearbArbOrt = bsBearbArbOrt;
	}

	/**
	 * @return the bsBearbFamName
	 */
	public String getBsBearbFamName() {
		return bsBearbFamName;
	}

	/**
	 * @param bsBearbFamName the bsBearbFamName to set
	 */
	public void setBsBearbFamName(String bsBearbFamName) {
		this.bsBearbFamName = bsBearbFamName;
	}

	/**
	 * @return the bsBearbKennung
	 */
	public String getBsBearbKennung() {
		return bsBearbKennung;
	}

	/**
	 * @param bsBearbKennung the bsBearbKennung to set
	 */
	public void setBsBearbKennung(String bsBearbKennung) {
		this.bsBearbKennung = bsBearbKennung;
	}

	/**
	 * @return the bsBearbKuerzel
	 */
	public String getBsBearbKuerzel() {
		return bsBearbKuerzel;
	}

	/**
	 * @param bsBearbKuerzel the bsBearbKuerzel to set
	 */
	public void setBsBearbKuerzel(String bsBearbKuerzel) {
		this.bsBearbKuerzel = bsBearbKuerzel;
	}

	/**
	 * @return the telephoneNumber
	 */
	public String getTelephoneNumber() {
		return telephoneNumber;
	}

	/**
	 * @param telephoneNumber the telephoneNumber to set
	 */
	public void setTelephoneNumber(String telephoneNumber) {
		this.telephoneNumber = telephoneNumber;
	}

	/**
	 * @return the firstName
	 */
	public String getFirstName() {
		return firstName;
	}

	/**
	 * @param firstName the firstName to set
	 */
	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}

	/**
	 * @return the password
	 */
	public String getPassword() {
		return password;
	}

	/**
	 * @param password the password to set
	 */
	public void setPassword(String password) {
		this.password = password;
	}


}
